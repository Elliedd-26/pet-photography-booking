@model Photography_Booking_System.Models.Booking_Service

@{
    ViewData["Title"] = "Booking Service Details";
}

<h1>Details</h1>

<div>
    <h4>Booking Service</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Booking ID</dt>
        <dd class="col-sm-10">@Model.BookingId</dd>

        <dt class="col-sm-2">Service Name</dt>
        <dd class="col-sm-10">@Model.Service?.Name</dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-bookingId="@Model.BookingId" asp-route-serviceId="@Model.ServiceId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
